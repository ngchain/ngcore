  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug help",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "env": {},
      "program": "${workspaceFolder}/cmd/ngcore",
      "args": [
        "help"
      ]
    },
    {
      "name": "debug gen check",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "env": {},
      "program": "${workspaceFolder}/cmd/ngcore",
      "args": [
        "gentools check"
      ]
    },
    {
      "name": "debug mining",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/ngcore",
      "env": {},
      "args": [
        "--mining",
        "1",
        "--log-level",
        "debug",
      ]
    },
    {
      "name": "debug",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "buildFlags": "-buildmode=exe",
      "program": "${workspaceFolder}/cmd/ngcore",
      "env": {},
      "args": [
        "--log-level",
        "debug",
      ]
    }
  ]
}